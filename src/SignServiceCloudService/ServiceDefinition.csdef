<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SignServiceCloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WebRole name="SignServiceWebRole" vmsize="Standard_D2_v2">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="Authentication__AzureAd__AADInstance" />
      <Setting name="Authentication__AzureAd__Audience" />
      <Setting name="Authentication__AzureAd__ClientId" />
      <Setting name="Authentication__AzureAd__TenantId" />
      <Setting name="Authentication__AzureAd__ClientSecret" />
      <Setting name="CertificateInfo__KeyVaultUrl" />
      <Setting name="CertificateInfo__KeyVaulCertificatename" />
      <Setting name="CertificateInfo__TimeStampUrl" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="https" port="443" certificate="Certificate1" />
    </Endpoints>
    <Certificates>
      <Certificate name="Certificate1" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
  </WebRole>
</ServiceDefinition>